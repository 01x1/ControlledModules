function ApplyCustom(a1,a2,a3,a4,a5,a6,a7)
    GetIdleAnimation(a1)
    GetIdle2Animation(a2)
    GetWalkAnimation(a3)
    GetRunAnimation(a4)
    GetJumpAnimation(a5)
    GetClimbAnimation(a6)
    GetFallAnimation(a7)
end
